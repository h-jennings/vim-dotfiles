{
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "scss",
    "css",
    "markdown",
    "html"
  ],
  "snippets.ultisnips.directories": ["UltiSnips", "~/.config/nvim/utils/snips"],
  "yank.highlight.duration": 300,
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"]
}
